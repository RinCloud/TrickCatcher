#include <iostream>
#include <vector>
using namespace std;

int main() {
    int H, W;
    cin >> H >> W;
    
    vector<string> image(H);
    for (int i = 0; i < H; i++) {
        cin >> image[i];
    }
    
    for (int i = 0; i < H; i++) {
        cout << image[i] << endl;
        cout << image[i] << endl;
    }
    
    return 0;
}