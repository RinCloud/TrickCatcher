#include <iostream>
#include <vector>
#include <string>

using namespace std;

int main() {
    // Read the height and width of the image
    int h, w;
    cin >> h >> w;

    // Create a vector of strings to represent the original image
    vector<string> image(h);
    for (int i = 0; i < h; i++) {
        cin >> image[i];
    }

    // Print the extended image
    for (int i = 0; i < h; i++) {
        cout << image[i] << endl;
        cout << image[i] << endl;
    }

    return 0;
}