#include <iostream>
#include <vector>
#include <string>

int main() {
    int H, W;
    std::cin >> H >> W;

    std::vector<std::string> image(H);
    for (int i = 0; i < H; i++) {
        std::cin >> image[i];
    }

    for (int i = 0; i < H; i++) {
        std::cout << image[i] << '\n' << image[i] << '\n';
    }

    return 0;
}