#include <iostream>
#include <string>

int main() {
    std::string S;
    std::cin >> S;

    int price = 700;
    if (S[0] == 'o') {
        price += 100;
    }
    if (S[1] == 'o') {
        price += 100;
    }
    if (S[2] == 'o') {
        price += 100;
    }

    std::cout << price << std::endl;

    return 0;
}