#include <iostream>
#include <vector>
#include <algorithm>
#include <cmath>
using namespace std;

int main() {
    // read input
    string N;
    cin >> N;

    // calculate the number of digits in N
    int numDigits = N.length();

    // calculate the minimum possible number of total banknotes
    int minBanknotes = numDigits;

    // print the result
    cout << minBanknotes << endl;

    return 0;
}