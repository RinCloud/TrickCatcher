#include <iostream>
#include <cmath>

using namespace std;

int main() {
    int a, b, c, d;
    cin >> a >> b >> c >> d;
    
    int distance1 = abs(c - a);
    int distance2 = abs(b - a);
    int distance3 = abs(c - b);
    
    if (distance1 <= d || (distance2 <= d && distance3 <= d)) {
        cout << "Yes" << endl;
    } else {
        cout << "No" << endl;
    }
    
    return 0;
}