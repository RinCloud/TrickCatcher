#include <iostream>

int main() {
    int N;
    std::cin >> N;

    for (int i = 1; i <= 9; i++) {
        for (int j = 1; j <= 9; j++) {
            if (i * j == N) {
                std::cout << "Yes";
                return 0;
            }
        }
    }

    std::cout << "No";
    return 0;
}