#include <iostream>
#include <cmath>
using namespace std;

int main() {
    int A, B;
    cin >> A >> B;

    bool flag = true;

    if (abs(A - B) <= 1) {
        flag = false;
    }

    if (A > 8 || B > 8) {
        flag = false;
    }

    if (flag) {
        cout << "Yay!";
    } else {
        cout << ":(";
    }

    return 0;
}