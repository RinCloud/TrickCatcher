#include <iostream>
#include <string>

int main() {
    std::string S, T, U;
    int A, B;
    
    // Read inputs
    std::cin >> S >> T;
    std::cin >> A >> B;
    std::cin >> U;
    
    // Update the counts based on the chosen ball
    if (U == S) {
        A--;
    } else {
        B--;
    }
    
    // Print the updated counts
    std::cout << A << " " << B;
    
    return 0;
}