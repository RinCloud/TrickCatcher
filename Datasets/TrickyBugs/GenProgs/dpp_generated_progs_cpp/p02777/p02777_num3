#include <iostream>
#include <string>
using namespace std;

int main() {
    string S, T, U;
    int A, B;
    
    // Input
    cin >> S >> T;
    cin >> A >> B;
    cin >> U;
    
    // Count number of balls with string S and T after removal
    int countS = (U == S) ? A - 1 : A;
    int countT = (U == T) ? B - 1 : B;
    
    // Output
    cout << countS << " " << countT << endl;

    return 0;
}