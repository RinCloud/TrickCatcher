#include <iostream>
#include <cmath>

int main() {
    int X;
    std::cin >> X;

    int time = 1;
    int distance = 0;

    while (true) {
        distance += time;
        if (distance >= X) {
            std::cout << time << std::endl;
            break;
        }
        time++;
    }

    return 0;
}