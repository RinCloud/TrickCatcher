#include <iostream>
#include <string>

int main() {
    int a, b;
    std::cin >> a >> b;

    std::string str1, str2;
    for (int i = 0; i < a; ++i) {
        str1 += std::to_string(b);
    }

    for (int i = 0; i < b; ++i) {
        str2 += std::to_string(a);
    }

    if (str1 < str2) {
        std::cout << str1;
    } else {
        std::cout << str2;
    }

    return 0;
}