#include <iostream>
#include <string>
using namespace std;

int main() {
    int a, b;
    cin >> a >> b;

    string strA, strB;
    for (int i = 0; i < a; i++) {
        strA += to_string(b);
    }

    for (int i = 0; i < b; i++) {
        strB += to_string(a);
    }

    if (strA < strB) {
        cout << strA << endl;
    } else {
        cout << strB << endl;
    }

    return 0;
}