#include <iostream>
#include <string>
using namespace std;

int main() {
    int N;
    cin >> N;
    string S;
    bool hasYellow = false;
    for (int i = 0; i < N; i++) {
        cin >> S;
        if (S == "Y") {
            hasYellow = true;
            break;
        }
    }
    if (hasYellow) {
        cout << "Four" << endl;
    } else {
        cout << "Three" << endl;
    }
    return 0;
}