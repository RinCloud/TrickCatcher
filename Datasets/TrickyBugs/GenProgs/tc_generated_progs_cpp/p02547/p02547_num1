```cpp
#include<bits/stdc++.h>
using namespace std;

int main(){
    int n, temp = 0;
    cin >> n;
    bool found = false;
    for(int i = 0; i < n; i++){
        int a, b;
        cin >> a >> b;
        if(a == b) temp++;
        else temp = 0;
        if(temp >= 2){
            cout << "Yes\n";
            found = true;
            break;
        }
    }
    if(!found) cout << "No\n";
}
```