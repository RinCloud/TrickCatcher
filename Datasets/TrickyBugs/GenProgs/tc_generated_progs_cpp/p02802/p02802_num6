```cpp
#include <bits/stdc++.h>
using namespace std;

int main() {
  int N, M;
  cin >> N >> M;
  vector<bool> K(N+1, false);
  vector<int> A(N+1, 0);
  int ac = 0;
  int wa = 0;
  
  for (int i = 0; i < M; i++) {
    int P;
    string S;
    cin >> P >> S;
    
    if (!K[P]) {
      if (S == "WA") {
        A[P]++;
      } else {
        ac++;
        wa += A[P];
        K[P] = true;
      }
    }
  }
  
  printf("%d %d", ac, wa);
}
```