def is_possible(S):
    # Count the occurrence of each direction
    north = S.count('N')
    west = S.count('W')
    south = S.count('S')
    east = S.count('E')
    
    # Check if the total north-south and west-east distance is zero
    if (north == south) and (west == east):
        return "Yes"
    else:
        return "No"

# Read the input string
S = input()

# Check if it is possible to set each day's travel distance
result = is_possible(S)

# Print the result
print(result)
