import math

# Getting input values
a, b = map(int, input().split())

# Concatenating a and b
concatenation = str(a) + str(b)

# Converting the concatenation to integer
concatenation = int(concatenation)

# Checking if the concatenation is a square number
is_square = math.isqrt(concatenation)**2 == concatenation

# Printing the result
if is_square:
    print("Yes")
else:
    print("No")