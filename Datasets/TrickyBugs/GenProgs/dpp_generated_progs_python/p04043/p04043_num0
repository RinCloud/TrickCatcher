# Program

def is_haiku_possible(a, b, c):
    if a == 5 and b == 7 and c == 5:
        return "YES"
    elif a == 7 and b == 5 and c == 7:
        return "NO"
    elif a == 5 and b == 5 and c == 7:
        return "YES"
    else:
        return "NO"

# Read input
a, b, c = map(int, input().split())

# Check if Haiku is possible
result = is_haiku_possible(a, b, c)

# Print output
print(result)