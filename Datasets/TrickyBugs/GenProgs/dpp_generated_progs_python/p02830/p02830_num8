# Get input from the user
N = int(input())
S, T = input().split()

# Initialize the new string
new_string = ""

# Loop through each index from 0 to N-1
for i in range(N):
    # Add the character from S at index i
    new_string += S[i]
    # Add the character from T at index i
    new_string += T[i]

# Print the new string
print(new_string)