# Read the input values
N = int(input())
K = int(input())

# Initialize count to 0
count = 0

# Loop through all the numbers from 1 to N
for num in range(1, N+1):
    # Find the number of non-zero digits in the current number
    non_zero_digits = str(num).count('0')

    # If the number of non-zero digits is equal to K, increment the count
    if non_zero_digits == K:
        count += 1

# Print the final count
print(count)