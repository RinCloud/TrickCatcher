# Reading input from standard input
N = int(input())

# Find the maximum possible sum of the digits
def max_digit_sum(N):
    # Convert the given number to string
    num_str = str(N)
    
    # Calculate the sum of the digits
    digit_sum = sum(int(digit) for digit in num_str)
    
    return digit_sum

# Print the maximum possible sum of the digits
print(max_digit_sum(N))