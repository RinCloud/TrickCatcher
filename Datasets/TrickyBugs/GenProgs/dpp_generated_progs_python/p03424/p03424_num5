import sys

def main():
    # read the input values
    N = int(input())
    arare_colors = input().split()

    # count the number of unique colors
    unique_colors = set(arare_colors)
    num_colors = len(unique_colors)

    # determine the output based on the number of colors
    if num_colors == 3:
        print("Three")
    elif num_colors == 4:
        print("Four")

if __name__ == '__main__':
    main()